using System;using Blaze;using UnityEngine;using System.Collections;using System.Collections.Generic;namespace Loonim{		/// <summary>	/// A raster texture which is applied to a surface.	/// Should generally be avoided as they're fixed size.	/// </summary>		public class RasterSurfaceTexture:SurfaceTexture{				/// <summary>The raw image.</summary>		public Texture2D Image;						/// <summary>Creates a raster surface texture with the given image.</summary>		public RasterSurfaceTexture(Texture2D image){						Image=image;					}			}	}